<p id="notice"><%= notice %></p>

<h1>Listing People</h1>
<%= link_to 'Home', :root%>

<div class="col-xs-10 col-xs-offset-1 col-sm-6 col-sm-offset-3 col-md-4 col-md-offset-4 panel panel-default">
<% @people.each do |person| %>
<div class="panel-body">
  <div class="panel-default">
    <%= best_in_place person, :name %>
  </div>
</div>
<% end %>
</div>
